{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "settings": {
        "prettier/prettier": ["error"],
        "import/resolver": {
            "webpack": {
                "config": "./webpack.config.js"
            }
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"]
    }
}